{
  "name": "aws-easy-deploy",
  "description": "A toolkit for deploying UIs and APIs with ease",
  "version": "0.0.0",
  "author": "Kerry Ritter @KerryRitter",
  "bin": {
    "aws-easy-deploy": "./bin/run"
